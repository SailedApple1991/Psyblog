{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/tailwind.css -o app/css/tailwind.css",
    "build:strapi-api": "swagger-typescript-api -p ../cms/src/extensions/documentation/documentation/1.0.0/full_documentation.json -o ./app/api -n strapi.ts",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app/tailwind.css -o app/css/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@remix-run/css-bundle": "^1.16.1",
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.16.1",
    "@use-gesture/react": "^10.2.27",
    "accept-language-parser": "^1.5.0",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "flowbite": "^1.6.5",
    "flowbite-react": "^0.4.7",
    "isbot": "^3.6.8",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-swipeable": "^7.0.1",
    "remix-auth": "^3.4.0",
    "remix-auth-form": "^1.3.0",
    "stripe": "^12.6.0",
    "swagger-typescript-api": "^12.0.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.16.1",
    "@remix-run/eslint-config": "^1.16.1",
    "@types/accept-language-parser": "^1.5.3",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/react-slick": "^0.23.10",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14"
  }
}
